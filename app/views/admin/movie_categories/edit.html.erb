<% content_for :css do %>
  <link href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" rel="stylesheet">
  <%= stylesheet_link_tag 'admin/movie_categories', media: "all", "data-turbolinks-track": "reload" %>
  <%= stylesheet_link_tag 'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>

<div class = "wrapper">
  <div class="main">
    <div class="container">
      <h1>Movie setting</h1> 
      <%= form_with url: admin_user_movie_category_path, local: true, html: { multipart: true, class: "edit_item"} do |form| %>
        <%= render partial: "form", locals: {form: form} %>
        <%= form.submit "COMMIT", class: "submit"%>
      <% end %>
    </div>
  </div>
  <%= render partial: "admin/admin"%>
</div>

<%= javascript_include_tag 'admin/movie_categories.js'%>