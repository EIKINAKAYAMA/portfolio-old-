<% content_for :css do %>
  <%= stylesheet_link_tag 'admin/designs', media: 'all', 'data-turbolinks-track': 'reload'%>
  <%= stylesheet_link_tag 'admin', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>
<%= render "admin/admin"%>

design_admin_new
<div class="contents row">
  <div class="container">
    <%= form_with model:[:admin, @user, @design], html: { multipart: true, class: "edit_item", remote: true}, local:true do |form| %>
      <h3>
        投稿する
      </h3>
      
      <%= render partial: "form", locals: {form: form} %>
      <%= form.submit "SEND"%>
    <% end %>
  </div>
</div>

<div id="instagram">
    <h2>instagram投稿</h2>
    <%= link_to 'Instagramで登録',
    "https://api.instagram.com/oauth/authorize?client_id=1055133514921533&redirect_uri=https%3A%2F%2Fmylifefolio.com%2F&scope=user_profile,user_media&response_type=code"%>
    <ul id="instagram-list">
    </ul>
</div>

<%= javascript_include_tag 'admin/designs.js'%>